<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
   <!-- tools:context="net.simplifiedcoding.locationsearchexample.MapsActivity"> -->
    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:map="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        />
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/rel2">
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/hospital"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="40dp"
            android:src="@drawable/ic_hospital"
            android:onClick="onclick"
            />
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/schools"
            android:layout_alignParentBottom="true"
            android:layout_toEndOf="@+id/restaurents"
            android:src="@drawable/ic_school"
            android:onClick="onclick"
            />
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/restaurents"
            android:onClick="onclick"
            android:layout_alignParentBottom="true"
            android:layout_toEndOf="@+id/hospital"
            android:src="@drawable/ic_restaurents"
            />
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/getroute"
            android:layout_alignParentBottom="true"
            android:layout_toEndOf="@id/schools"
            android:src="@drawable/ic_getroute"
            android:onClick="onclick"
            />
    </RelativeLayout>
</RelativeLayout>